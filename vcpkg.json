{
    "$schema":
        "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
    "name": "metier",
    "version": "24.2.0",
    "features": {
        "qt": {
            "description": "Use Qt provided by vcpkg",
            "dependencies": [
                {
                    "name": "opentxs",
                    "features": [
                        "qt6"
                    ]
                },
                {
                    "name": "qtbase",
                    "default-features": false,
                    "features": [
                        "gui",
                        "widgets"
                    ]
                },
                "qtdeclarative",
                "qtsvg"
            ]
        }
    },
    "dependencies": [
        "boost",
        "cmakerc",
        "opentxs"
    ]
}
